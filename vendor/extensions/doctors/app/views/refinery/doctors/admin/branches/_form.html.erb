<%= form_for [refinery, :doctors_admin, @branch] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @branch,
              :include_object_name => true %>

  
  <div class='field'>
    <%= f.label :name -%>
    <%= f.text_field :name, :class => 'larger widest'  -%>

  </div>
  
  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.branches.admin.branches.branch'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @branch.name) %>
<% end -%>
